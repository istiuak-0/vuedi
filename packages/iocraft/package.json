{
  "name": "iocraft",
  "version": "0.2.3",
  "description": "A lightweight IOC container for Vue 3 using the Composition API.",
  "keywords": [
    "vue",
    "vue3",
    "dependency-injection",
    "di",
    "ioc",
    "Inversion of Control",
    "composition-api",
    "typescript",
    "vue-plugin"
  ],
  "homepage": "https://github.com/istiuak-0/iocraft#readme",
  "bugs": {
    "url": "https://github.com/istiuak-0/iocraft/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istiuak-0/iocraft.git",
    "directory": "packages/iocraft"
  },
  "license": "MIT",
  "author": {
    "name": "Istiuak",
    "url": "https://github.com/istiuak-0"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.js",
      "require": "./dist/helpers.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.16",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}